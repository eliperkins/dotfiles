{
  "features": {
    // "edit_prediction_provider": "zed"
  },
  "context_servers": {
    "mcp-server-context7": {
      "source": "extension",
      "settings": {
        "default_minimum_tokens": "10000"
      }
    }
  },
  "agent": {
    "always_allow_tool_actions": true,
    "default_profile": "write",
    "default_model": {
      "provider": "lmstudio",
      "model": "gpt-oss-120b"
    },
    "inline_assistant_model": {
      "provider": "lmstudio",
      "model": "devstral-small-2505-mlx"
    }
  },
  "languages": {
    "Markdown": {
      "soft_wrap": "editor_width"
    },
    "JavaScript": {
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      }
    },
    "Swift": {
      "language_servers": ["sourcekit-lsp"],
      "formatter": "language_server",
      "format_on_save": "on",
      "tab_size": 2
    },
    "GraphQL": {
      "tab_size": 2,
      "formatter": "language_server",
      "format_on_save": "on"
    }
  },
  "buffer_font_family": "Dank Mono",
  "buffer_font_size": 16,
  "terminal": {
    "font_size": 14,
    "font_family": "Dank Mono"
  },
  "theme": "Night Owl Dark",
  "scrollbar": {
    "show": "auto"
  },
  "experimental.theme_overrides": {
    "scrollbar.track.background": "#0000",
    "scrollbar.track.border": "#0000",
    "terminal.foreground": "#eff0eb",
    "terminal.ansi.black": "#2f3b54",
    "terminal.ansi.bright_black": "#2f3b54",
    "terminal.ansi.red": "#ff5c57",
    "terminal.ansi.bright_red": "#ff5c57",
    "terminal.ansi.green": "#5af78e",
    "terminal.ansi.bright_green": "#5af78e",
    "terminal.ansi.yellow": "#f3f99d",
    "terminal.ansi.bright_yellow": "#f3f99d",
    "terminal.ansi.blue": "#57c7ff",
    "terminal.ansi.bright_blue": "#57c7ff",
    "terminal.ansi.magenta": "#ff6ac1",
    "terminal.ansi.bright_magenta": "#ff6ac1",
    "terminal.ansi.cyan": "#9aedfe",
    "terminal.ansi.bright_cyan": "#9aedfe",
    "terminal.ansi.white": "#f1f1f0",
    "terminal.ansi.bright_white": "#f1f1f0"
  },
  "file_scan_exclusions": ["node_modules", ".git/**"],
  "auto_install_extensions": {
    "dockerfile": true,
    "git-firefly": true,
    "graphql": true,
    "html": true,
    "make": true,
    "night-owlz": true,
    "ruby": true,
    "sql": true,
    "swift": true,
    "terraform": true,
    "toml": true,
    "xml": true
  }
}
